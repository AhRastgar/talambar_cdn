{
  "name": "@jibres/siftal-v3",
  "version": "3.0.0",
  "description": "Siftal for Jibres - v3",
  "main": "tailwind.config.js",
  "dependencies": {},
  "keywords": [
    "jibres",
    "CSS",
    "Siftal",
    "tailwind"
  ],
  "author": "Javad Adib",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.0.12",
    "postcss": "^8.4.12",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.23"
  },
  "scripts": {
    "tw-dev": "NODE_ENV=development npx tailwindcss -i ./tw/tailwind.css -o ./dist/tw-personalised-v3.scss --watch",
    "tw-build": "NODE_ENV=production npx tailwindcss -i ./tw/tailwind.css -o ./dist/tw-personalised-v3.min.scss --watch --minify",
    "magic-dev": "sass --watch magic/magic.scss:dist/magic-v3.css",
    "magic-build": "sass --watch magic/magic.scss:dist/magic-v3.min.css --style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jibres/talambar_cdn.git"
  },
  "bugs": {
    "url": "https://github.com/jibres/talambar_cdn/issues"
  },
  "homepage": "https://github.com/jibres/talambar_cdn#readme",
  "publishConfig": {
    "access": "public"
  }
}
